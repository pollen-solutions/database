{
  "name": "pollen-solutions/database",
  "description": "Pollen Solutions - Database Component - Database toolkit for PHP",
  "homepage": "https://www.presstify.com/pollen-solutions/database/",
  "keywords": [
    "pollen-solutions",
    "component",
    "database",
    "eloquent"
  ],
  "time": "2021-08-12",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/dbal": "^3.1",
    "illuminate/database": "^8.0",
    "illuminate/pagination": "^8.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Database\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Database\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Solutions - Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
